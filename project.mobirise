          "_customHTML": "<section class=\"form\" group=\"Form\" plugins=\"formstyler, datepicker\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"range\" inline title=\"Felső térköz\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline title=\"Alsó térköz\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <header>Style</header>\n        <input type=\"checkbox\" name=\"customStyle\" title=\"Custom Style\" checked>\n        <input type=\"checkbox\" name=\"disableElements\" title=\"Disable Control\">\n        <input type=\"color\" name=\"fieldColor\" title=\"Field\" value=\"#ffffff\" condition=\"customStyle\">\n        <input type=\"color\" name=\"fieldHover\" title=\"Field Hover\" value=\"#efefef\" condition=\"customStyle\">\n        <input type=\"color\" name=\"borderColor\" title=\"Border\" value=\"#cccccc\" condition=\"customStyle\">\n        <input type=\"color\" name=\"borderHover\" title=\"Border Hover\" value=\"#cccccc\" condition=\"customStyle\">\n        <input type=\"color\" name=\"formText\" title=\"Szöveg\" value=\"#000000\" condition=\"customStyle\">\n        <input type=\"color\" name=\"formFocusText\" title=\"Text Focus\" value=\"#232323\" condition=\"customStyle\">\n        <input type=\"color\" name=\"formPlaceholder\" title=\"Placeholders\" value=\"#c1c1c1\" condition=\"customStyle\">\n        <input type=\"color\" name=\"formFocusPlaceholder\" title=\"Placeholders Focus\" value=\"#c1c1c1\" condition=\"customStyle\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Háttérkép\" value=\"@PROJECT_PATH@/assets/images/mbr-2.jpg\" selected>\n            <input type=\"color\" title=\"Háttérszín\" value=\"#b2ccd2\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Fedés\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Fedőszín\" name=\"overlayColor\" value=\"#e5eef0\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Átlátszóság\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.9\" condition=\"overlay && bg.type !== 'color'\">\n    <input type=\"hidden\" name=\"usedRecaptcha\" value=\"false\"></mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay && bg.type=='image')\"></div>\n    <div class=\"container\">\n        <div class=\"row\">\n            <div mbr-form class=\"col-lg-8 mx-auto mbr-form\">\n<!--Formbuilder Form-->\n<form action=\"info@virtual-office-bratislava.com\" name=\"Online\" method=\"POST\" class=\"mbr-form form-with-styler\">\n<div class=\"form-row\">\n<div hidden=\"hidden\" data-form-alert class=\"alert alert-success col-12\">Ďakujeme za vyplnenie, náš kolega Vás čoskoro kontaktuje e-mailom!</div>\n<div hidden=\"hidden\" data-form-alert-danger class=\"alert alert-danger col-12\">Oops...! some problem!</div>\n</div>\n<div class=\"dragArea form-row\">\n<div class=\"col-lg-12 col-md-12 col-sm-12\">\n<h4 mbr-theme-style=\"display-5\" class=\"mbr-fonts-style\">Rýchle uzatvorenie zmluvy online – naši kolegovia Vám zašlú vypracovanú zmluvu v priebehu niekoľkých minút. Nevzniká Vám tým žiadny záväzok.\n</h4>\n</div>\n<div class=\"col-lg-12 col-md-12 col-sm-12\">\n<hr>\n</div>\n<div data-for=\"Názov firmy \" class=\"col-lg-12 col-md-12 col-sm-12 form-group\">\n<input type=\"text\" name=\"Názov firmy \" placeholder=\"Názov spoločnosti / živnostník*\" data-form-field=\"Názov firmy \" mbr-theme-style=\"display-7\" class=\"form-control\" required=\"required\" value>\n</div>\n<div data-for=\"email\" class=\"col-lg-12 col-md-12 col-sm-12 form-group\">\n<input type=\"email\" name=\"email\" placeholder=\"Email*\" data-form-field=\"email\" mbr-theme-style=\"display-7\" class=\"form-control\" required=\"required\" value>\n</div>\n<div data-for=\"Telefón\" class=\"col-lg-12 col-md-12 col-sm-12 form-group\">\n<input type=\"tel\" name=\"Telefón\" placeholder=\"Telefón*\" pattern=\"*\" data-form-field=\"Telefón\" mbr-theme-style=\"display-7\" class=\"form-control\" required=\"required\" value>\n</div>\n<div data-for=\"Konateľ\" class=\"col-lg-12 col-md-12 col-sm-12 form-group\">\n<input type=\"text\" name=\"Konateľ\" placeholder=\"Konateľ\" data-form-field=\"Konateľ\" mbr-theme-style=\"display-7\" required=\"required\" class=\"form-control\" value>\n</div>\n<div class=\"col-lg-12 col-md-12 col-sm-12 form-group\" data-for=\"Adresa na doručenie pošty (iba pri prémiovom balíku)\">\n<input type=\"text\" name=\"Adresa na doručenie pošty (iba pri prémiovom balíku)\" placeholder=\"Adresa na doručenie pošty (iba pri prémiovom balíku)\" data-form-field=\"Adresa na doručenie pošty (iba pri prémiovom balíku)\" mbr-theme-style=\"display-7\" class=\"form-control\" value>\n</div>\n<div data-for=\"Zvolený balík:\" class=\"col-lg-12 col-md-12 col-sm-12 form-group\">\n<div class=\"form-control-label\">\n<label mbr-text mbr-theme-style=\"display-7\" for=\"Zvolený balík:\" class=\"mbr-fonts-style\">Zvolený balík:&nbsp;</label>\n</div>\n<div class=\"form-check\">\n<input type=\"radio\" name=\"Zvolený balík:\" data-form-field=\"Zvolený balík:\" mbr-theme-style=\"display-7\" class=\"form-check-input\" value=\"Základný balík 6 mesiacov -- 66 €\" checked>\n<label mbr-text mbr-theme-style=\"display-7\" class=\"form-check-label\">Základný balík 6 mesiacov -- 66 €</label>\n</div>\n<div class=\"form-check\">\n<input type=\"radio\" name=\"Zvolený balík:\" data-form-field=\"Zvolený balík:\" mbr-theme-style=\"display-7\" class=\"form-check-input\" value=\"Základný balík 12 mesiacov -- 132 €\">\n<label mbr-text mbr-theme-style=\"display-7\" class=\"form-check-label\">Základný balík 12 mesiacov -- 132 €</label>\n</div>\n<div class=\"form-check\">\n<input type=\"radio\" name=\"Zvolený balík:\" data-form-field=\"Zvolený balík:\" mbr-theme-style=\"display-7\" class=\"form-check-input\" value=\"